<Window x:Class="AllocatorExampleGUI.ParamWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AllocatorExampleGUI"
        mc:Ignorable="d"
        Title="ParamWindow" Height="250" Width="300" MaxWidth="300" MinWidth="300" Loaded="Window_Loaded">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="40"/>
        </Grid.RowDefinitions>
        <ScrollViewer Name="SwParams" Grid.Row="0"/>
        <Button Name="BtnCreate" Content="Create" Grid.Row="1" Click="BtnCreate_Click"/>
    </Grid>
</Window>
